{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tcadf"
		},
		"blobinfogit_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobinfogit'"
		},
		"info_sql_unibenpocdb_git_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'info_sql_unibenpocdb_git'"
		},
		"info_unibenpocdb_sql_git_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'info_unibenpocdb_sql_git'"
		},
		"infosqldbgit_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'infosqldbgit'"
		},
		"infosqlunibenpocdbgit_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'infosqlunibenpocdbgit'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tcadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blobinfogit_connectionString": {
						"value": "[parameters('blobinfogit_connectionString')]"
					},
					"info_sql_unibenpocdb_git_connectionString": {
						"value": "[parameters('info_sql_unibenpocdb_git_connectionString')]"
					},
					"info_unibenpocdb_sql_git_connectionString": {
						"value": "[parameters('info_unibenpocdb_sql_git_connectionString')]"
					},
					"infosqldbgit_connectionString": {
						"value": "[parameters('infosqldbgit_connectionString')]"
					},
					"infosqlunibenpocdbgit_connectionString": {
						"value": "[parameters('infosqlunibenpocdbgit_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}